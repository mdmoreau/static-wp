// automatically generated from svgstore-sass task template
.svgstore {
  display: block;
  position: relative;

  &::before {
    display: block;
    content: '';
  }

  svg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
  }
}
{{ #items }}

$svgstore--{{ name }}: {{{ data }}};

.svgstore--{{ name }} {
  &::before {
    padding-bottom: {{ height }} / {{ width }} * 100%;
  }
}
{{ /items }}
